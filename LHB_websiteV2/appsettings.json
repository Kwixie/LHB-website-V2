{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Umbraco": {
    "Storage": {
      "AzureBlob": {
        "Media": {
          "ConnectionString": "Umbraco:Storage:AzureBlob:Media:ConnectionString",
          "ContainerName": "lhb-storage"
        }
      }
    },
    "CMS": {
      "Global": {
        "Id": "43d3a1d4-1e42-4cc5-9eee-df129a220fdf",
        "SanitizeTinyMce": true
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      },
      "ModelsBuilder": {
        "ModelsMode": "SourceCodeAuto",
        "ModelsDirectory": "~/umbraco/models",
        "AcceptUnsafeModelsDirectory": true
      },
      "Unattended": {
        "UpgradeUnattended": true
      },
      "Security": {
        "AllowConcurrentLogins": false
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "ConnectionStrings:umbracoDbDSN",
    "umbracoDbDSN_ProviderName": "Microsoft.Data.SqlClient"
  }
}